    return 0;
}
(part - B) (Compare price)
#include <iostream>
#include <string>

using namespace std;

class Book {
private:
    string book_name;
    float price;
    int no_of_pages;

public:
    void accept_data() {
        cin.ignore();
        cout << "Enter Book Name: ";
        getline(cin, book_name);
        
        cout << "Enter Price: ";
        cin >> price;
        
        cout << "Enter Number of Pages: ";
        cin >> no_of_pages;
    }

    float get_price() const {
        return price;
    }

    string get_book_name() const {
        return book_name;
    }
};

int main() {
    Book book1, book2;

    cout << "--- Enter Data for Book 1 ---\n";
    book1.accept_data();

    cout << "\n--- Enter Data for Book 2 ---\n";
    cin.ignore(); 
    book2.accept_data();
    
    cout << "\n--- Price Comparison ---\n";
    if (book1.get_price() > book2.get_price()) {
        cout << "**Book with greater price is: " << book1.get_book_name() << "** (Price: " << book1.get_price() << ")\n";
    } else if (book2.get_price() > book1.get_price()) {
        cout << "**Book with greater price is: " << book2.get_book_name() << "** (Price: " << book2.get_price() << ")\n";
    } else {
        cout << "Both books have the same price.\n";
    }

    return 0;
}
(part - c) (Convert to seconds)
#include <iostream>
#include <string>
#include <sstream>

using namespace std;

class Time {
private:
    int hours;
    int minutes;
    int seconds;
    long total_seconds;

public:
    void accept_time() {
        string time_str;
        cout << "Enter time in HH:MM:SS format (e.g., 05:30:15): ";
        cin >> time_str;

        char colon;
        stringstream ss(time_str);
        
        if (ss >> hours >> colon && colon == ':' && 
            ss >> minutes >> colon && colon == ':' && 
            ss >> seconds) {
            
            if (hours >= 0 && minutes >= 0 && minutes < 60 && seconds >= 0 && seconds < 60) {
                cout << "Time accepted: " << hours << "h " << minutes << "m " << seconds << "s\n";
            } else {
                cout << "Invalid time components entered. Setting to 00:00:00.\n";
                hours = minutes = seconds = 0;
            }
        } else {
            cout << "Invalid time format. Setting to 00:00:00.\n";
            hours = minutes = seconds = 0;
        }
    }

    void convert_to_seconds() {
        total_seconds = (long)hours * 3600 + (long)minutes * 60 + (long)seconds;
    }

    void display_total_seconds() {
        cout << "\n--- Conversion Result ---\n";
        cout << "The time (" << hours << ":" << minutes << ":" << seconds << ") is equivalent to:\n";
        cout << "**Total Seconds: " << total_seconds << "**\n";
    }
};

int main() {
    Time t1;
    t1.accept_time();
    t1.convert_to_seconds();
    t1.display_total_seconds();

    return 0;
}
(Polymorphism) ( 2. part A)
#include <iostream>
#include <string>
#include <cstring>

using namespace std;

class CustomString {
private:
    char str[100];
public:
    CustomString() {
        str[0] = '\0';
    }
    CustomString(const char s[]) {
        strcpy(str, s);
    }
    
    void display() const {
        cout << str;
    }

    // Overload Binary '+' operator for concatenation
    CustomString operator+ (const CustomString& other) {
        CustomString result;
        strcpy(result.str, str);
        strcat(result.str, other.str);
        return result;
    }
};

int main() {
    CustomString s1("xyz");
    CustomString s2("pqr");
    CustomString s3;

    cout << "String 1: ";
    s1.display();
    cout << "\nString 2: ";
    s2.display();

    // Calls s1.operator+(s2)
    s3 = s1 + s2; 

    cout << "\n\nResult of concatenation (String 1 + String 2):\n";
    cout << "Output: **";
    s3.display();
    cout << "**\n";

    return 0;
}
(Part - b)
#include <iostream>
#include <string>

using namespace std;

// Base Class (ILogin)
class ILogin {
protected:
    string name;
    string password;

public:
    ILogin() : name(""), password("") {}
    
    // Virtual function for Run-time Polymorphism
    virtual void accept() {
        cout << "Enter Name: ";
        cin >> name;
        cout << "Enter Password: ";
        cin >> password;
    }

    void display_base() const {
        cout << "Name: " << name << "\n";
        cout << "Password: " << password << "\n";
    }
    
    // Virtual destructor is good practice for base classes
    virtual ~ILogin() {}
};

// Derived Class 1
class EmailLogin : public ILogin {
private:
    string email_provider;
public:
    // Override the virtual accept function
    void accept() override {
        cout << "\n--- Email Login Details ---\n";
        ILogin::accept();
        cout << "Enter Email Provider (e.g., gmail): ";
        cin >> email_provider;
    }
    
    void display() const {
        display_base();
        cout << "Provider: " << email_provider << "\n";
    }
};

// Derived Class 2
class MembershipLogin : public ILogin {
private:
    string membership_type;
public:
    // Override the virtual accept function
    void accept() override {
        cout << "\n--- Membership Login Details ---\n";
        ILogin::accept();
        cout << "Enter Membership Type (e.g., Premium): ";
        cin >> membership_type;
    }
    
    void display() const {
        display_base();
        cout << "Type: " << membership_type << "\n";
    }
};

int main() {
    EmailLogin email_emp;
    MembershipLogin member_emp;
    
    // Pointers to Base Class
    ILogin* ptr_email = &email_emp;
    ILogin* ptr_member = &member_emp;

    // Run-time Polymorphism: The call resolves to the derived class's accept()
    ptr_email->accept(); 
    ptr_member->accept();

    cout << "\n========================================\n";
    cout << "--- DISPLAYING EMPLOYEE LOGIN DETAILS ---\n";
    cout << "========================================\n";
    
    email_emp.display();
    
    cout << "\n";
    member_emp.display();

    return 0;
}
Exp - 9 ( part - A)
#include <iostream>
#include <fstream>
#include <string>

using namespace std;

void create_first_file_if_not_exists() {
    ofstream ofile("First.txt", ios::out);
    if (ofile.is_open()) {
        ofile << "This is the content of the first file.\n";
        ofile << "It is being copied to the second file.\n";
        ofile.close();
        cout << "Note: 'First.txt' created with sample content for demonstration.\n";
    }
}

int main() {
    create_first_file_if_not_exists();

    ifstream fin("First.txt", ios::in);
    ofstream fout("Second.txt", ios::out);

    if (!fin.is_open()) {
        cerr << "Error: Could not open First.txt for reading.\n";
        return 1;
    }

    if (!fout.is_open()) {
        cerr << "Error: Could not open Second.txt for writing.\n";
        fin.close();
        return 1;
    }

    char ch;
    while (fin.get(ch)) {
        fout.put(ch);
    }

    fin.close();
    fout.close();

    cout << "\nFile Copy Successful!\n";
    cout << "Contents of 'First.txt' copied to 'Second.txt'.\n";

    return 0;
}
(part - b)
#include <iostream>
#include <fstream>
#include <cctype>

using namespace std;

int main() {
    // Note: Creating a sample file for demonstration
    ofstream sample_out("sample.txt");
    sample_out << "My address is 123 Main Street. The year is 2024. Total 5 digits and 7 spaces here.\n";
    sample_out.close();

    ifstream fin("sample.txt");
    if (!fin.is_open()) {
        cerr << "Error: Could not open sample.txt.\n";
        return 1;
    }

    char ch;
    int digit_count = 0;
    int space_count = 0;

    while (fin.get(ch)) {
        if (isdigit(ch)) {
            digit_count++;
        } else if (isspace(ch)) {
            space_count++;
        }
    }

    fin.close();

    cout << "\n--- Count Digits and Spaces ---\n";
    cout << "Total Digits found: " << digit_count << "\n";
    cout << "Total Spaces found: " << space_count << "\n";

    return 0;
}
Exp - 10 (Combined Code)
#include <iostream>
#include <string>
#include <limits> // Needed for numeric_limits in Part c

using namespace std;

// --- Part a: Sum of Numbers Using Parameterized Constructor (SumCalculator) ---
class SumCalculator {
private:
    int n_limit;
    long total_sum;

public:
    // Parameterized Constructor
    SumCalculator(int n) {
        n_limit = n;
        total_sum = 0;
        
        for (int i = 1; i <= n_limit; ++i) {
            total_sum += i;
        }
    }

    void display_sum() {
        cout << "Sum of numbers from 1 to " << n_limit << " is: **" << total_sum << "**\n";
    }
};

// --- Part b: Initializing Data Members with Parameterized Constructor (student) ---
class student {
private:
    string name;
    float percentage;

public:
    // Parameterized Constructor
    student(string n, float p) {
        name = n;
        percentage = p;
    }

    void display_data() {
        cout << "Name: " << name << "\n";
        cout << "Percentage: " << percentage << "%\n";
    }
};

// --- Part c: Constructor with Default Argument (College) ---
class College {
private:
    int roll_no;
    string name;
    string course;

public:
    // Constructor with default argument for 'course'
    College(int r, string n, string c = "Computer Engineering") {
        roll_no = r;
        name = n;
        course = c;
    }

    void display_data() {
        cout << "  Roll No: " << roll_no << "\n";
        cout << "  Name: " << name << "\n";
        cout << "  Course: **" << course << "**\n";
    }
};

// --- Part d: Constructor Overloading (Point) ---
class Point {
private:
    int x;
    int y;

public:
    // 1. Default Constructor (No arguments)
    Point() {
        x = 0;
        y = 0;
    }

    // 2. Parameterized Constructor (Two arguments)
    Point(int x_val, int y_val) {
        x = x_val;
        y = y_val;
    }

    // 3. Parameterized Constructor (One argument)
    Point(int val) {
        x = val;
        y = val;
    }

    void display_coordinates(const string& obj_name) {
        cout << obj_name << " Coordinates: (" << x << ", " << y << ")\n";
    }
};

// =======================================================
//                       MAIN FUNCTION
// =======================================================
int main() {

    // --- DEMONSTRATION 5a: Sum of Numbers using Parameterized Constructor ---
    cout << "=================================================\n";
    cout << "5a) Sum of Numbers (1 to N) using Parameterized Constructor\n";
    cout << "=================================================\n";
    int input_n = 10;
    cout << "Calculating sum for N = " << input_n << "\n";
    SumCalculator sc(input_n);
    sc.display_sum();
    cout << "\n";

    // --- DEMONSTRATION 5b: Student Data Initialization using Parameterized Constructor ---
    cout << "=================================================\n";
    cout << "5b) Student Data Initialization using Parameterized Constructor\n";
    cout << "=================================================\n";
    string s_name = "Alice Smith";
    float s_perc = 85.5f;
    cout << "Initializing student object with name: " << s_name << ", percentage: " << s_perc << "%\n";
    student s1(s_name, s_perc);
    s1.display_data();
    cout << "\n";

    // --- DEMONSTRATION 5c: Constructor with Default Argument ---
    cout << "=================================================\n";
    cout << "5c) Constructor with Default Argument (Course)\n";
    cout << "=================================================\n";

    // Object 1: Uses the default course ("Computer Engineering")
    College c1(101, "Bob Johnson"); 
    cout << "Object 1 (Default Course):\n";
    c1.display_data();
    
    // Object 2: Overrides the default course
    College c2(102, "Charlie Brown", "Electrical Engineering"); 
    cout << "Object 2 (Specified Course):\n";
    c2.display_data();
    cout << "\n";
    
    // --- DEMONSTRATION 5d: Constructor Overloading ---
    cout << "=================================================\n";
    cout << "5d) Constructor Overloading (Point Class)\n";
    cout << "=================================================\n";

    // Calls Default Constructor (No args)
    Point p1;
    p1.display_coordinates("P1 (Default)"); 

    // Calls Parameterized Constructor (Two args)
    Point p2(15, 25);
    p2.display_coordinates("P2 (Two Args)");

    // Calls Parameterized Constructor (One arg)
    Point p3(7);
    p3.display_coordinates("P3 (One Arg)");

    return 0;
Exp - 11 
#include <iostream>
#include <vector>
#include <algorithm>

template <class T>
class GenericVector {
private:
    std::vector<T> data;

public:
    // To create the vector (Constructor)
    GenericVector() {}

    // Function to initialize the vector elements
    void createVector(const std::vector<T>& initialData) {
        data = initialData;
    }

    // a) To modify the value of a given element
    bool modifyElement(int index, const T& newValue) {
        if (index >= 0 && index < data.size()) {
            data[index] = newValue;
            return true;
        }
        return false;
    }

    // b) To multiply by a scalar value (Assumes T supports multiplication with T)
    void multiplyByScalar(const T& scalar) {
        for (size_t i = 0; i < data.size(); ++i) {
            data[i] = data[i] * scalar;
        }
    }

    // c) To display the vector in the form (10,20,30,...)
    void displayVector() const {
        std::cout << "(";
        for (size_t i = 0; i < data.size(); ++i) {
            std::cout << data[i];
            if (i < data.size() - 1) {
                std::cout << ",";
            }
        }
        std::cout << ")" << std::endl;
    }
};

int main() {
    using namespace std;

    cout << "--- Integer Vector Test ---" << endl;
    GenericVector<int> intVec;
    
    vector<int> initialInts = {10, 20, 30, 40};
    intVec.createVector(initialInts);
    
    cout << "Initial Vector: ";
    intVec.displayVector();

    // a) Modify element at index 1 (20 -> 25)
    if (intVec.modifyElement(1, 25)) {
        cout << "After modifying element at index 1: ";
    } else {
        cout << "Modification failed (invalid index). Current Vector: ";
    }
    intVec.displayVector();

    // b) Multiply by scalar 2
    intVec.multiplyByScalar(2);
    cout << "After multiplying by scalar 2: ";
    intVec.displayVector();
    
    cout << "--- Double Vector Test ---" << endl;
    GenericVector<double> doubleVec;
    
    vector<double> initialDoubles = {1.5, 2.5, 3.5};
    doubleVec.createVector(initialDoubles);
    
    cout << "Initial Vector: ";
    doubleVec.displayVector();

    // a) Modify element at index 0 (1.5 -> 5.0)
    doubleVec.modifyElement(0, 5.0);
    cout << "After modifying element at index 0: ";
    doubleVec.displayVector();

    // b) Multiply by scalar 1.5
    doubleVec.multiplyByScalar(1.5);
    cout << "After multiplying by scalar 1.5: ";
    doubleVec.displayVector();
    
    return 0;
}
Exp - 12
#include <iostream>
#include <stack>
#include <string>

void implementStack() {
    using namespace std;

    cout << "--- STL Stack Implementation ---" << endl;
    stack<string> bookStack;

    bookStack.push("Book A");
    bookStack.push("Book B");
    bookStack.push("Book C");

    cout << "Current stack size: " << bookStack.size() << endl;

    cout << "Top element: " << bookStack.top() << endl;

    bookStack.pop();
    cout << "Popped one element." << endl;

    cout << "New top element: " << bookStack.top() << endl;

    bookStack.pop();
    bookStack.pop();

    if (bookStack.empty()) {
        cout << "Stack is now empty." << endl;
    }
}

