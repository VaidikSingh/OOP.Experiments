Exp - 1) Part - A
#include <iostream>
#include <string>

using namespace std;

// Define the 'student' class
class student {
private:
    int roll_no;
    string name;
    string student_class;

public:
    // Function to accept student data
    void accept_data() {
        cout << "Enter Roll Number: ";
        cin >> roll_no;
        
        cin.ignore();
        
        cout << "Enter Name: ";
        
        getline(cin, name);
        
        cout << "Enter Class: ";
        getline(cin, student_class);
    }

    // Function to display student data
    void display_data() {
        cout << "\n--- Student Details ---\n";
        cout << "Roll Number: " << roll_no << "\n";
        cout << "Name: " << name << "\n";
        cout << "Class: " << student_class << "\n";
    }
};

int main() {
    // Declare an object of the student class
    student s1;

    cout << "--- Enter Data for One Student ---\n";
    // Call the member function to accept data
    s1.accept_data();

    // Call the member function to display data
    s1.display_data();

    return 0;
}
(part - B)
#include <iostream>
#include <string>

using namespace std;

// Define the 'Book' class
class Book {
private:
    string book_name;
    float price;
    int no_of_pages;

public:
    // Function to accept book data
    void accept_data() {
        cin.ignore();
        cout << "Enter Book Name: ";
        getline(cin, book_name);
        
        cout << "Enter Price: ";
        cin >> price;
        
        cout << "Enter Number of Pages: ";
        cin >> no_of_pages;
    }

    // Function to display book data
    void display_data() {
        cout << "\nBook Name: " << book_name;
        cout << "\nPrice: " << price;
        cout << "\nNumber of Pages: " << no_of_pages << "\n";
    }

    // Function to get the price of the book
    float get_price() const {
        return price;
    }

    // Function to get the name of the book
    string get_book_name() const {
        return book_name;
    }
};

int main() {
    // Declare two objects of the Book class
    Book book1, book2;

    cout << "--- Enter Data for Book 1 ---\n";
    book1.accept_data();

    cout << "\n--- Enter Data for Book 2 ---\n";
    
    cin.ignore();
    book2.accept_data();
    
    cout << "\n--- Book Details ---\n";
    cout << "\nBook 1:";
    book1.display_data();
    cout << "\nBook 2:";
    book2.display_data();

    // Compare prices and display the name of the book with the greater price
    cout << "\n--- Price Comparison ---\n";
    if (book1.get_price() > book2.get_price()) {
        cout << "**The book with the greater price is: " << book1.get_book_name() << "** (Price: " << book1.get_price() << ")\n";
    } else if (book2.get_price() > book1.get_price()) {
        cout << "**The book with the greater price is: " << book2.get_book_name() << "** (Price: " << book2.get_price() << ")\n";
    } else {
        cout << "Both books have the same price.\n";
    }

    return 0;
}
(part - C)
#include <iostream>
#include <string>
#include <sstream>

using namespace std;

// Define the 'time' class
class Time {
private:
    int hours;
    int minutes;
    int seconds;
    long total_seconds;

public:
    // Function to accept time in HH:MM:SS format
    void accept_time() {
        string time_str;
        cout << "Enter time in HH:MM:SS format (e.g., 05:30:15): ";
        cin >> time_str;

        char colon;
        stringstream ss(time_str);
        
        if (ss >> hours >> colon && colon == ':' && 
            ss >> minutes >> colon && colon == ':' && 
            ss >> seconds) {
            
            if (hours >= 0 && minutes >= 0 && minutes < 60 && seconds >= 0 && seconds < 60) {
                cout << "Time accepted successfully: " << hours << "h " << minutes << "m " << seconds << "s\n";
            } else {
                cout << "Invalid time components entered. Setting to 00:00:00.\n";
                hours = minutes = seconds = 0;
            }
        } else {
            cout << "Invalid time format. Setting to 00:00:00.\n";
            hours = minutes = seconds = 0;
        }
    }

    // Function to convert time to total seconds
    void convert_to_seconds() {
        
        total_seconds = (long)hours * 3600 + (long)minutes * 60 + (long)seconds;
    }

    // Function to display total seconds
    void display_total_seconds() {
        cout << "\n--- Conversion Result ---\n";
        cout << "The time (" << hours << ":" << minutes << ":" << seconds << ") is equivalent to:\n";
        cout << "**Total Seconds: " << total_seconds << "**\n";
    }
};

int main() {
    // Declare an object of the Time class
    Time t1;

    t1.accept_time();
    
    t1.convert_to_seconds();
    
    t1.display_total_seconds();

    return 0;
}
Exp - 2) (part - A)
#include <iostream>
#include <string>
#include <limits>

using namespace std;

const int NUM_CITIES = 5;

// Define the 'city' class
class city {
private:
    string name;
    long population;

public:
    // Function to accept city data
    void accept_data() {
        cin.ignore(numeric_limits<streamsize>::max(), '\n');
        cout << "Enter City Name: ";
        getline(cin, name);
        
        cout << "Enter Population: ";
        cin >> population;
    }

    // Function to display city data
    void display_data() {
        cout << "Name: " << name << ", Population: " << population << "\n";
    }

    // Function to get the population
    long get_population() const {
        return population;
    }

    // Function to get the name
    string get_name() const {
        return name;
    }
};

int main() {
    // Declare an array of 5 city objects
    city city_array[NUM_CITIES];

    cout << "--- Enter Data for " << NUM_CITIES << " Cities ---\n";
    for (int i = 0; i < NUM_CITIES; ++i) {
        cout << "\nEnter Details for City " << i + 1 << ":\n";
        city_array[i].accept_data();
    }

    // Find the city with the highest population
    long max_population = 0;
    string city_with_max_pop;

    for (int i = 0; i < NUM_CITIES; ++i) {
        if (city_array[i].get_population() > max_population) {
            max_population = city_array[i].get_population();
            city_with_max_pop = city_array[i].get_name();
        }
    }

    cout << "\n--- All City Details ---\n";
    for (int i = 0; i < NUM_CITIES; ++i) {
        city_array[i].display_data();
    }

    cout << "\n--- Result ---\n";
    cout << "**The city with the highest population is: " << city_with_max_pop << "** (Population: " << max_population << ")\n";

    return 0;
}
(part - b)
#include <iostream>
#include <iomanip>

using namespace std;

const int NUM_ACCOUNTS = 10;

// Define the 'Account' class
class Account {
private:
    long account_no;
    double balance;

public:
    // Function to accept account data
    void accept_data(int index) {
        cout << "Enter Account No. for Account " << index << ": ";
        cin >> account_no;
        
        cout << "Enter Balance for Account " << index << ": ";
        cin >> balance;
    }

    // Function to calculate and add interest
    void give_interest() {
        if (balance >= 5000.0) {
            double interest = balance * 0.10;
            balance += interest;
        }
    }

    // Function to display account data
    void display_data() {
        cout << fixed << setprecision(2);
        cout << "A/C No: " << account_no << ", New Balance: â‚¹" << balance << "\n";
    }
};

int main() {
    // Declare an array of 10 Account objects
    Account account_array[NUM_ACCOUNTS];

    cout << "--- Enter Data for " << NUM_ACCOUNTS << " Accounts ---\n";
    for (int i = 0; i < NUM_ACCOUNTS; ++i) {
        cout << "\nEnter Details for Account " << i + 1 << ":\n";
        account_array[i].accept_data(i + 1);
    }

    cout << "\n--- Applying 10% Interest to Eligible Accounts (Balance >= 5000) ---\n";
    for (int i = 0; i < NUM_ACCOUNTS; ++i) {
        account_array[i].give_interest();
    }

    cout << "\n--- Updated Account Details ---\n";
    for (int i = 0; i < NUM_ACCOUNTS; ++i) {
        account_array[i].display_data();
    }

    return 0;
}
(part- c)
#include <iostream>
#include <string>
#include <limits>
#include <algorithm>

using namespace std;

const int NUM_STAFF = 5;

// Define the 'Staff' class
class Staff {
private:
    string name;
    string post;

public:
    // Function to accept staff data
    void accept_data() {
        cin.ignore(numeric_limits<streamsize>::max(), '\n');
        cout << "Enter Staff Name: ";
        getline(cin, name);
        
        cout << "Enter Post (e.g., HOD, Teacher, Clerk): ";
        getline(cin, post);
    }

    // Function to get the staff name
    string get_name() const {
        return name;
    }

    // Function to check if the staff is HOD
    bool is_hod() const {
        // Convert post to uppercase for case-insensitive comparison
        string temp_post = post;
        transform(temp_post.begin(), temp_post.end(), temp_post.begin(), ::toupper);
        
        return temp_post == "HOD";
    }
};

int main() {
    // Declare an array of 5 Staff objects
    Staff staff_array[NUM_STAFF];

    cout << "--- Enter Data for " << NUM_STAFF << " Staff Members ---\n";
    for (int i = 0; i < NUM_STAFF; ++i) {
        cout << "\nEnter Details for Staff Member " << i + 1 << ":\n";
        staff_array[i].accept_data();
    }

    cout << "\n--- Staff Members Who Are HOD ---\n";
    bool found_hod = false;
    for (int i = 0; i < NUM_STAFF; ++i) {
        if (staff_array[i].is_hod()) {
            cout << "**" << staff_array[i].get_name() << "**\n";
            found_hod = true;
        }
    }

    if (!found_hod) {
        cout << "No staff member found with the post 'HOD'.\n";
    }

    return 0;
}
Exp - 3) (Part - A)
#include <iostream>
#include <string>
#include <limits>

using namespace std;

// Define the 'book' class
class book {
private:
    string book_title;
    string author_name;
    float price;

public:
    // Function to accept book data
    void accept_info() {
        cin.ignore(numeric_limits<streamsize>::max(), '\n');
        cout << "Enter Book Title: ";
        getline(cin, book_title);
        
        cout << "Enter Author Name: ";
        getline(cin, author_name);
        
        cout << "Enter Price: ";
        cin >> price;
    }

    // Function to display book data
    void display_info() {
        cout << "\n--- Book Information ---\n";
        cout << "Title: " << book_title << "\n";
        cout << "Author: " << author_name << "\n";
        cout << "Price: " << price << "\n";
    }
};

int main() {
    // Declare an object of the book class
    book b1;
    
    // Declare a pointer to a book object and make it point to b1
    book *ptr_b1;
    ptr_b1 = &b1;

    cout << "--- Accepting Data Using Pointer ---\n";
    // Invoke the member function using the arrow operator (->)
    ptr_b1->accept_info();

    cout << "\n--- Displaying Data Using Pointer ---\n";
    // Invoke the member function using the arrow operator (->)
    ptr_b1->display_info();

    return 0;
}
(part- b)
#include <iostream>

using namespace std;

// Define the 'student' class
class student {
private:
    int roll_no;
    float percentage;

public:
    // Function to accept student data
    void accept_data(int roll, float perc) {
        // Use 'this' pointer to distinguish between member variables and parameters
        this->roll_no = roll;
        this->percentage = perc;
    }

    // Function to display student data
    void display_data() {
        cout << "\n--- Student Details (Using this pointer) ---\n";
        // Use 'this' pointer to explicitly call another member function
        this->print_details();
    }
    
    // Helper function to print details
    void print_details() {
        cout << "Roll No: " << roll_no << "\n";
        cout << "Percentage: " << percentage << "%\n";
    }
};

int main() {
    // Declare an object of the student class
    student s1;
    int roll;
    float perc;

    cout << "Enter Roll Number: ";
    cin >> roll;
    cout << "Enter Percentage: ";
    cin >> perc;

    // Invoke accept_data
    s1.accept_data(roll, perc);

    // Invoke display_data which internally uses 'this'
    s1.display_data();

    return 0;
}
(part - c)
#include <iostream>
#include <string>

using namespace std;

// Define the Outer Class
class Person {
private:
    string name;

    // Define the Nested Class (Inner Class)
    class Address {
    public:
        string city;
        string country;

        void input_address() {
            cout << "Enter City: ";
            cin >> city;
            cout << "Enter Country: ";
            cin >> country;
        }

        void display_address() {
            cout << "City: " << city << "\n";
            cout << "Country: " << country << "\n";
        }
    }; 
    
    // Create an object of the nested class within the outer class
    Address residence;

public:
    void input_details() {
        cout << "--- Enter Person Details ---\n";
        cout << "Enter Name: ";
        cin >> name;
        
        cout << "\n--- Enter Address Details ---\n";
        residence.input_address();
    }

    void display_details() {
        cout << "\n--- Full Details ---\n";
        cout << "Name: " << name << "\n";
        cout << "Address:\n";
        residence.display_address();
    }
};

int main() {
    // Declare an object of the Outer Class
    Person p1;

    p1.input_details();
    p1.display_details();

    return 0;
}
Exp - 4 (Part- A)
#include <iostream>

using namespace std;

// Define the first class
class result1 {
private:
    float marks1;

public:
    void read_marks() {
        cout << "Enter marks for Result 1: ";
        cin >> marks1;
    }

    float get_marks() const {
        return marks1;
    }
};

// Define the second class
class result2 {
private:
    float marks2;

public:
    void read_marks() {
        cout << "Enter marks for Result 2: ";
        cin >> marks2;
    }

    float get_marks() const {
        return marks2;
    }
};

// Non-member function to calculate average, accepting objects as arguments
void compute_average(result1 r1_obj, result2 r2_obj) {
    float total_marks = r1_obj.get_marks() + r2_obj.get_marks();
    float average = total_marks / 2.0;

    cout << "\n--- Calculation ---\n";
    cout << "Total Marks: " << total_marks << "\n";
    cout << "Average of two results: " << average << "\n";
}

int main() {
    result1 res1;
    result2 res2;

    cout << "--- Enter Data ---\n";
    res1.read_marks();
    res2.read_marks();

    // Passing objects 'res1' and 'res2' to the function
    compute_average(res1, res2);

    return 0;
}
(part - b)
#include <iostream>

using namespace std;

// Forward declaration of ClassB is necessary because find_greatest uses it
class ClassB;

// Define ClassA
class ClassA {
private:
    int num_a;

public:
    ClassA() : num_a(0) {}

    void read_data() {
        cout << "Enter a number for Class A: ";
        cin >> num_a;
    }

    // Declare the non-member function as a friend
    friend void find_greatest(ClassA a, ClassB b);
};

// Define ClassB
class ClassB {
private:
    int num_b;

public:
    ClassB() : num_b(0) {}

    void read_data() {
        cout << "Enter a number for Class B: ";
        cin >> num_b;
    }

    // Declare the non-member function as a friend
    friend void find_greatest(ClassA a, ClassB b);
};

// Definition of the friend function
void find_greatest(ClassA a, ClassB b) {
    cout << "\n--- Comparison ---\n";
    if (a.num_a > b.num_b) {
        cout << "The greatest number is from Class A: " << a.num_a << "\n";
    } else if (b.num_b > a.num_a) {
        cout << "The greatest number is from Class B: " << b.num_b << "\n";
    } else {
        cout << "Both numbers are equal: " << a.num_a << "\n";
    }
}

int main() {
    ClassA obj_a;
    ClassB obj_b;

    obj_a.read_data();
    obj_b.read_data();

    find_greatest(obj_a, obj_b);

    return 0;
}
(part - c)
#include <iostream>

using namespace std;

// Define the class
class Data {
private:
    int value;

public:
    Data() : value(0) {}

    void set_value(int v) {
        value = v;
    }

    void display_value(const string& obj_name) const {
        cout << obj_name << " value: " << value << "\n";
    }

    // Declare the non-member function as a friend
    friend void swap_values(Data& d1, Data& d2);
};

// Definition of the friend function
// Note: Objects are passed by reference (&) to allow modification
void swap_values(Data& d1, Data& d2) {
    int temp = d1.value;
    d1.value = d2.value;
    d2.value = temp;

    cout << "\n--- Swap Successful ---\n";
}

int main() {
    Data obj1;
    Data obj2;
    int v1, v2;

    cout << "Enter value for Object 1: ";
    cin >> v1;
    obj1.set_value(v1);

    cout << "Enter value for Object 2: ";
    cin >> v2;
    obj2.set_value(v2);

    cout << "\n--- Before Swap ---\n";
    obj1.display_value("Object 1");
    obj2.display_value("Object 2");

    // Calling the friend function to swap the values
    swap_values(obj1, obj2);

    cout << "\n--- After Swap ---\n";
    obj1.display_value("Object 1");
    obj2.display_value("Object 2");

    return 0;
}
Exp - 5 (part - a)
#include <iostream>

using namespace std;

class SumCalculator {
private:
    int n_limit;
    long total_sum;

public:
    // Parameterized Constructor
    SumCalculator(int n) {
        n_limit = n;
        total_sum = 0;
        
        // Calculate the sum within the constructor
        for (int i = 1; i <= n_limit; ++i) {
            total_sum += i;
        }
        cout << "Sum calculated by the constructor for N=" << n_limit << "\n";
    }

    // Function to display the result
    void display_sum() {
        cout << "\n--- Sum Result ---\n";
        cout << "Sum of numbers from 1 to " << n_limit << " is: **" << total_sum << "**\n";
    }
};

int main() {
    int input_n;
    cout << "Enter the value of N: ";
    cin >> input_n;

    // Create an object, passing 'input_n' to the parameterized constructor
    SumCalculator sc(input_n);

    sc.display_sum();

    return 0;
}
